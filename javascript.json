	"Time Default Format": {
		"prefix": "time",
		"body": [
			"function timeDefaultFormat(time) {",
			"  let split = time.split(':');",
			"  timeFormat = split[0] + ':' + split[1];",
			"  return timeFormat;",			
			"}"			
		],
		"description": "Hora em formato padrão"
	},
	"Date Default Format": {
		"prefix": "date",
